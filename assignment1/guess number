
import random

number = random.randint(1, 100)

number_of_guesses = 0

while number_of_guesses < 10:
    print('Guess a number between 1 and 100:')

    guess = input()
    guess = int(guess)

    number_of_guesses = number_of_guesses + 1

    if guess == number:
        print (number)
    else:
        print ("your number not matching")
